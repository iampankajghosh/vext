    "panelTitle.activeForeground": "#ededed"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#aaaaaa",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": ["string"],
      "settings": {
        "foreground": "#6ac67c"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#e96a9f"
      }
    },
    {
      "name": "Functions & Methods",
      "scope": [
        "entity.name.function",
        "support.function",
        "variable.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#bf7af0"
      }
    },
    {
      "name": "Classes & Types",
      "scope": [
        "entity.name.type",
        "support.class",
        "support.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#52a8ff"
      }
    },
    {
      "name": "Properties & Member Variables",
      "scope": ["variable.object.property", "meta.field.declaration"],
      "settings": {
        "foreground": "#a0c7ff"
      }
    },
    {
      "name": "Variables & Parameters",
      "scope": ["variable", "variable.parameter"],
      "settings": {
        "foreground": "#d6deeb"
      }
    },
    {
      "name": "Constants & Numbers",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#f78e74"
      }
    },
    {
      "name": "Operators & Punctuation",
      "scope": ["keyword.operator", "punctuation"],
      "settings": {
        "foreground": "#a1a1a1"
      }
    },
    {
      "name": "Tags",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#62c073"
      }
    },
    {
      "name": "JSX / TSX Component Tags",
      "scope": [
        "entity.name.tag.jsx",
        "entity.name.tag.tsx",
        "support.class.component.jsx",
        "support.class.component.tsx"
      ],
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#bf7af0"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": {
        "foreground": "#ff6166",
        "fontStyle": "italic underline"
      }
    },
    {
      "name": "Markdown Headings & Links",
      "scope": ["markup.heading", "markup.underline.link", "string.other.link"],
      "settings": {
        "foreground": "#52a8ff"
      }
    }
  ],
  "semanticHighlighting": true
}
